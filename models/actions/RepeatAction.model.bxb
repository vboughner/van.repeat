action (RepeatAction) {
  type (Constructor)
  description (Repeats what was said)
  collect {
    input (repeatInput) {
      type (RepeatInput)
      min (Required) max (One)
    }
    input (voiceStyle) {
      type (VoiceStyle)
      min (Optional) max (One)
    }
  }
  output (RepeatResponse)
}
